<% if(alerts.error) { %>
  <% alerts.error.forEach(function(msg){ %>
    <div class="row" id="alert_box">
      <div class="col s12 m12">
        <div class="card red lighten-4">
          <%= msg %>
          <i class="material-icons icon-red right" id="alert_close" aria-hidden="true">close</i>
        </div>
      </div>
    </div>
  <% }); %>
<% } %>
<% if(alerts.success) { %>
  <% alerts.success.forEach(function(msg){ %>
    <div class="row" id="alert_box">
      <div class="col s12 m12">
        <div class="card green lighten-4">
          <%= msg %>
          <i class="material-icons icon-green right" id="alert_close" aria-hidden="true">check</i>
        </div>
      </div>
    </div>
  <% }); %>
<% } %>
